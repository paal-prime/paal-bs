EXPERIMENTS		:=	facility_location/ls_convergence tsp/mcts_policies_comparison
RESULTS_DIR		:=	results/

RESULTS       :=	$(addprefix $(RESULTS_DIR), $(EXPERIMENTS))

results: $(RESULTS)

$(RESULTS) : $(RESULTS_DIR)% : experiments/% results-tests
	$< $@

results-tests: UflLib TSPLIB FLClustered

clean-results:
	rm -rf $(RESULTS_DIR)

distclean: clean-results

.PHONY: results clean-results
